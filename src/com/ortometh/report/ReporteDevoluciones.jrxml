	<parameter name="idVenta" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     devoluciones."id_venta" AS devoluciones_id_venta,
     devoluciones."cantidad" AS devoluciones_cantidad,
     devoluciones."tipo" AS devoluciones_tipo,
     devoluciones."nombre_cliente" AS devoluciones_nombre_cliente,
     devoluciones."fecha" AS devoluciones_fecha,
     devoluciones."fecha_entrega" AS devoluciones_fecha_entrega,
     venta."id_venta" AS venta_id_venta,
     venta_producto."cantidad" AS venta_producto_cantidad,
     producto."clave_producto" AS producto_clave_producto,
     devoluciones."comentario" AS devoluciones_comentario,
     usuario."nombre" AS usuario_nombre
FROM
     "dbo"."venta" venta INNER JOIN "dbo"."devoluciones" devoluciones ON venta."id_venta" = devoluciones."id_venta"
     INNER JOIN "dbo"."venta_producto" venta_producto ON venta."id_venta" = venta_producto."id_venta"
     INNER JOIN "dbo"."producto" producto ON venta_producto."id_producto" = producto."id_producto"
     INNER JOIN "dbo"."usuario" usuario ON devoluciones."id_usuario" = usuario."id_usuario"
WHERE
     devoluciones.id_venta = $P{idVenta}]]>
	</queryString>
