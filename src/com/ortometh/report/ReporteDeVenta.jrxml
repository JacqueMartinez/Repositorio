	<parameter name="NumeroVenta" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     venta."id_venta" AS venta_id_venta,
     venta."tipo_venta" AS venta_tipo_venta,
     venta."fecha_venta" AS venta_fecha_venta,
     venta."importe" AS venta_importe,
     venta."descuento" AS venta_descuento,
     venta."usuario" AS venta_usuario,
     venta."fecha_umodificacion" AS venta_fecha_umodificacion,
     venta."tipoPago" AS venta_tipoPago,
     venta_producto."id_venta" AS venta_producto_id_venta,
     venta_producto."cantidad" AS venta_producto_cantidad,
     venta_producto."precio" AS venta_producto_precio,
     venta_producto."descuento" AS venta_producto_descuento,
     clientes."id_cliente" AS clientes_id_cliente,
     clientes."nombre" AS clientes_nombre,
     clientes."apellidop" AS clientes_apellidop,
     clientes."apellidom" AS clientes_apellidom,
     clientes."telefono" AS clientes_telefono,
     clientes."tipo_cliente" AS clientes_tipo_cliente,
     clientes."rfc" AS clientes_rfc,
     categoria_producto."id_categoria" AS categoria_producto_id_categoria,
     categoria_producto."nombre" AS categoria_producto_nombre,
     producto."id_categoria" AS producto_id_categoria,
     producto."marca" AS producto_marca,
     producto."descripcion" AS producto_descripcion,
     producto."clave" AS producto_clave,
     producto."precio_venta" AS producto_precio_venta,
     venta."comentarioPago" AS venta_comentarioPago
FROM
     "dbo"."venta" venta INNER JOIN "dbo"."venta_producto" venta_producto ON venta."id_venta" = venta_producto."id_venta"
     INNER JOIN "dbo"."clientes" clientes ON venta."id_cliente" = clientes."id_cliente"
     INNER JOIN "dbo"."producto" producto ON venta_producto."id_producto" = producto."id_producto"
     INNER JOIN "dbo"."categoria_producto" categoria_producto ON producto."id_categoria" = categoria_producto."id_categoria" WHERE venta."id_venta"=$P{NumeroVenta}]]>
	</queryString>
