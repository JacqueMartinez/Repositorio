	<parameter name="id_cliente" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     clientes."id_cliente" AS clientes_id_cliente,
     clientes."nombre" AS clientes_nombre,
     clientes."apellidop" AS clientes_apellidop,
     clientes."apellidom" AS clientes_apellidom,
     clientes."domicilio" AS clientes_domicilio,
     clientes."telefono" AS clientes_telefono,
     clientes."correo" AS clientes_correo,
     clientes."tipo_cliente" AS clientes_tipo_cliente,
     clientes."nombre_facturacion" AS clientes_nombre_facturacion,
     clientes."rfc" AS clientes_rfc,
     clientes."estado" AS clientes_estado,
     venta."id_venta" AS venta_id_venta,
     venta."id_cliente" AS venta_id_cliente,
     venta."tipo_venta" AS venta_tipo_venta,
     venta."fecha_venta" AS venta_fecha_venta,
     venta."entrada_venta" AS venta_entrada_venta,
     venta."importe" AS venta_importe,
     venta."descuento" AS venta_descuento,
     venta."usuario" AS venta_usuario,
     venta."fecha_umodificacion" AS venta_fecha_umodificacion,
     venta."tipoPago" AS venta_tipoPago,
     venta."referenciaPago" AS venta_referenciaPago,
     venta."comentarioPago" AS venta_comentarioPago,
     venta."estado" AS venta_estado,
     venta_producto."id_venta" AS venta_producto_id_venta,
     venta_producto."id_producto" AS venta_producto_id_producto,
     venta_producto."cantidad" AS venta_producto_cantidad,
     venta_producto."precio" AS venta_producto_precio,
     venta_producto."descuento" AS venta_producto_descuento,
     producto."id_producto" AS producto_id_producto,
     producto."clave_producto" AS producto_clave_producto,
     producto."id_categoria" AS producto_id_categoria,
     producto."id_tipo" AS producto_id_tipo,
     producto."id_proveedor" AS producto_id_proveedor,
     producto."clave" AS producto_clave,
     producto."marca" AS producto_marca,
     producto."descripcion" AS producto_descripcion,
     producto."stock" AS producto_stock,
     producto."stock_min" AS producto_stock_min,
     producto."defectuoso" AS producto_defectuoso,
     producto."precio_venta" AS producto_precio_venta,
     producto."precio_compra" AS producto_precio_compra,
     producto."estado" AS producto_estado
FROM
     "dbo"."clientes" clientes INNER JOIN "dbo"."venta" venta ON clientes."id_cliente" = venta."id_cliente"
     INNER JOIN "dbo"."venta_producto" venta_producto ON venta."id_venta" = venta_producto."id_venta"
     INNER JOIN "dbo"."producto" producto ON venta_producto."id_producto" = producto."id_producto"
where 
venta."id_cliente"=$P{id_cliente} AND venta."estado"='Finalizado']]>
	</queryString>
